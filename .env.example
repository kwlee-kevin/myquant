# Backend / Frontend
DATABASE_URL=postgresql://myquant:myquant@postgres:5432/myquant
DJANGO_SECRET_KEY=replace-with-secure-secret
BRIDGE_API_KEY=replace-with-bridge-shared-key
NEXT_PUBLIC_API_BASE=http://localhost:8000

# Kiwoom mode + credentials
KIWOOM_MODE=paper
KIWOOM_REAL_APP_KEY=replace-me
KIWOOM_REAL_APP_SECRET=replace-me
